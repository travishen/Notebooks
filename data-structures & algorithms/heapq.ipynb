   "source": [
    "from heapq import heappop, heappush, heapify\n",
    "\n",
    "heap = []\n",
    "nums = [12, 3, -5, 3, 6, 11]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "[-5, 3, 3, 12, 6, 11]"
      ]
     },
     "execution_count": 2,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "for n in nums:\n",
    "    heappush(heap, n)\n",
    "    \n",
    "heapify(heap)\n",
    "\n",
    "heap"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
